# FixitAI Environment Variables
# Copy this file to .env and fill in your actual values

# AI Services
GEMINI_API_KEY=your_gemini_api_key_here
MCP_SERVER_URL=your_mcp_server_url_here
OPENAI_API_KEY=your_openai_api_key_here

# Database
DATABASE_URL=your_database_connection_string
DATABASE_NAME=fixitai_db

# Storage
STORAGE_BUCKET=your_storage_bucket_name
STORAGE_REGION=your_storage_region

# Location Services
LOCATION_API_KEY=your_location_api_key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Social & Authentication
AUTH_SECRET=your_auth_secret_key
JWT_SECRET=your_jwt_secret_key

# External APIs
WEB_SCRAPING_API_KEY=your_web_scraping_api_key
VOICE_TO_TEXT_API_KEY=your_voice_api_key

# App Configuration
APP_ENV=development
DEBUG_MODE=true
LOG_LEVEL=info

# Server Configuration
PORT=3000
HOST=localhost
